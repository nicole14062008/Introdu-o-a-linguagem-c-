LETRA A)
#include <stdio.h>
 #include <stdlib.h>           
int main() {
   printf("********** \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("*        * \n");
    printf("********** \n");
    //7 por 5
    printf(" *** \n");
    printf("*   * \n");
    printf("*   * \n");
    printf("*   * \n");
    printf("*   * \n");
    printf("*   * \n");
    printf(" *** \n");
    //9 por 7
    printf("    * \n");
    printf("   *** \n");
    printf("  ***** \n");
    printf(" ******* \n");
    printf("********* \n");
    printf("   * \n");
    printf("   * \n");
    //O losangulo
    printf("-------------\n");
    printf("      *    \n");
    printf("    *  *  \n");
    printf("   *    *");
    printf("  *      * \n");
    printf("   *    * \n");
    printf("    *  * \n");
    printf("      *") ;
  return 0;
}

